source ~/dotfiles/zsh/aliases_files/git

# svn
alias ss='svn status'
alias slg='svn log -l 10'
alias sa='svn add'
alias sd='svn diff'
alias sr='svn revert'
alias srr='svn revert --recursive'
alias sc='svn commit'

# shortened
alias s='sudo'
alias e='emacs -nw'
alias ll='ls -lah'

# extra
alias m='mpv'
alias getip='curl -s icanhazip.com'

# directories
alias to='cd /mnt/extra/torrent'
alias ...='cd ../..'
alias ....='cd ../../..'

# current projects
alias pinc='cd ~/devel/pinbird-client'
alias pins='cd ~/workspace/pinbird'
alias no='cd ~/devel/notesdude'
# python
# Virtual Environment stuff
alias ve='source .env/bin/activate'
alias de='deactivate'

# Devel
alias ru='./bin/run.sh'
alias rut='./bin/run_tests.sh'
alias ruit='./bin/run_integration_tests.sh'
alias rukt='./bin/run_karma_tests.sh'
alias js='jekyll serve -w'

# Misc
alias fu='sudo $(history -p \!\!)'
alias beacons='ll /dev/tty.usbmodem*'
alias cwd='pwd | pbcopy'
alias psg='ps aux | grep -v grep | grep -i -e VSZ -e'

# find
alias fhere='find . -name '
alias flinks='find . -type l -ls'